package kr.or.ddit.util.validate.exception;

public class TenancyException extends RuntimeException{
    public TenancyException() {
        super("이전에 요청한 값이 있습니다.");  // 기본 메시지
    }

    public TenancyException(String message) {
        super(message);  // 사용자 정의 메시지
    }

    public TenancyException(String message, Throwable cause) {
        super(message, cause);  // 예외 원인 포함
    }

    public TenancyException(Throwable cause) {
        super(cause);  // 예외 원인만
    }
}
